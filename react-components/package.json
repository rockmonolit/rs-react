{
  "name": "react-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "builder": "eslint . --ext .tsx && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .tsx",
    "lint:fix": "eslint ./src --ext .tsx --fix",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "devop": "ts-node server.ts",
    "build:client": "vite build --outDir ../dist/client",
    "build:server": "vite build --outDir ../dist/server --ssr ServerApp.tsx",
    "build": "npm run build:client && npm run build:server",
    "builds": "vite build",
    "start": "ts-node --esm server.ts",
    "cypress:open": "cypress open",
    "cypress:e2e": "cypress run --e2e",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:e2e",
    "cypress:coverage": "npx nyc report --reporter=text-summary"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "*.{js,ts,css,md,tsx}": "prettier --write",
    "*.html": [
      "eslint",
      "prettier --write"
    ],
    "*.ts": "eslint --cache --fix"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/app.{ts,tsx}",
      "<rootDir>/src/components/*.{ts,tsx}",
      "<rootDir>/src/components/**/*.{ts,tsx}",
      "<rootDir>/src/slices/*.{ts,tsx}",
      "<rootDir>/src/store/*.{ts,tsx}",
      "<rootDir>/src/helpers/*.{ts,tsx}",
      "<rootDir>/src/mocks/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "text"
    ],
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/",
      "<rootDir>/instrumented/",
      "instrumented",
      "dist",
      "coverage",
      "cypress"
    ]
  },
  "ts-node": {
    "esm": true
  },
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/user-event": "^14.4.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-tsconfig-paths": "^4.2.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@cypress/code-coverage": "^3.10.4",
    "@redux-devtools/core": "^3.13.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/whatwg-fetch": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-preset-react-app": "^10.0.1",
    "cypress": "^12.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.30.1"
  }
}
